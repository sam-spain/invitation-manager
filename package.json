{
  "name": "invitation-manager",
  "version": "1.0.0",
  "description": "Personal project to create wedding invitation",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:server && node dist/index.js",
    "dev": "nodemon --exec babel-node server/index",
    "test": "npm run test-server && npm run test-client",
    "test-server": "jest '/server' --testEnvironment=node",
    "test-client": "jest '/client'",
    "build:client": "webpack --mode=production",
    "build:server": "babel server/ --out-dir dist/ --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-spain/invitation-manager.git"
  },
  "author": "sam-spain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sam-spain/invitation-manager/issues"
  },
  "homepage": "https://github.com/sam-spain/invitation-manager#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^26.0.20",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.11",
    "morgan": "^1.10.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "cli": "^1.0.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.4.2",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon": "^2.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "superagent": "^6.1.0",
    "supertest": "^5.0.0",
    "tailwindcss": "^2.0.2",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "webpack": "^5.12.3",
    "webpack-cli": "^4.3.1",
    "webpack-dev-middleware": "^4.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
